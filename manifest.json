{
  "manifest_version": 2,
  "name": "Cursor Changer",
  "description": "This extension allows a user to change their cursor to fit more of their style",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "content_scripts": [
    {
      "matches":["http://*/*"],
      "js":["jquery.js" ,"popup.js"]
    }
  ]
}
